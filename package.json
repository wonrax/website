{
  "name": "wonrax-web",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/hhai.dev",
    "api"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "wireit",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint"
  },
  "wireit": {
    "dev": {
      "dependencies": [
        "./apps/hhai.dev:dev",
        "./api:dev"
      ]
    }
  },
  "devDependencies": {
    "@prisma/client": "^5.8.0",
    "prettier": "latest",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.2",
    "turbo": "^1.11.2",
    "wireit": "^0.14.1"
  },
  "packageManager": "npm@9.5.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "prisma": {
    "seed": "ts-node api/prisma/seed.ts"
  }
}
